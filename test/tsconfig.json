{
    "compilerOptions": {
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": true,
        "target": "es5",
        "lib": [
            "es2015",
            "dom"
        ],
        "sourceMap": true,
        "module": "es2015",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "outDir": "./",
        "declaration": false
    },
    "exclude": []
}